	<Frame name="WQT_SettingsBaseTemplate" mixin="WQT_SettingsBaseMixin" virtual="true">
		<KeyValues>
			<KeyValue key="tooltipOffsetX" type="number" value="4"/>
			<KeyValue key="tooltipOffsetY" type="number" value="-20"/>
		</KeyValues>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="BgHighlight" atlas="socialqueuing-row-select" alphaMode="ADD" alpha="0.1" hidden="true"/>
			</Layer>
		</Layers>

		<Frames>
			<Frame parentKey="DisabledOverlay" inherits="WQT_SettingsChildTemplate" setAllPoints="true" hidden="true"/>
			<Frame parentKey="NewFeature" hidden="true">
				<Size x="24" y="24" />
				<Anchors>
					<Anchor point="LEFT" x="12" y="-1" />
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Icon" atlas="wowlabs_spellbucketicon-sword" setAllPoints="true" />
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						GameTooltip:SetOwner(self, "ANCHOR_RIGHT", -4, -4);
						GameTooltip:SetText((NEW_CAPS:lower():gsub("^%l", string.upper)));
						GameTooltip:Show();
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Frame>
		</Frames>

		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEnter>
				self:OnEnter(self);
			</OnEnter>
			<OnLeave>
				self:OnLeave();
			</OnLeave>
		</Scripts>
	</Frame>

	<Frame name="WQT_SettingsChildTemplate" virtual="true">
		<Scripts>
			<OnEnter>
					self.parent:OnEnter(self.parent);
			</OnEnter>
			<OnLeave>
					self.parent:OnLeave();
			</OnLeave>
		</Scripts>
	</Frame>

	<Button name="WQT_SettingCheckboxTemplate" inherits="WQT_SettingsBaseTemplate" mixin="WQT_SettingsCheckboxMixin" virtual="true">
		<Size y="32"/>	
		<Frames>
			<CheckButton parentKey="CheckBox" inherits="WQT_SettingsChildTemplate" enableMouse="false">
				<Size x="25" y="25"/>
				<Anchors>
					<Anchor point="LEFT" x="40"/>
				</Anchors>
				<NormalTexture atlas="checkbox-minimal" useAtlasSize="true" />
				<PushedTexture atlas="checkbox-minimal" useAtlasSize="true" />
				<CheckedTexture atlas="checkmark-minimal" useAtlasSize="true" />
				<DisabledCheckedTexture atlas="checkmark-minimal-disabled" useAtlasSize="true" />
				<Scripts>
					<OnClick>
						self:GetParent():OnValueChanged(self:GetChecked(), true);
						PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
					</OnClick>
				</Scripts>
			</CheckButton>

			
		</Frames>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormal" maxLines="1" justifyH="LEFT">
					<Anchors>
						<Anchor point="LEFT" relativeKey="$parent.CheckBox" relativePoint="RIGHT" x="5"/>
						<Anchor point="RIGHT" x="-15"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>

		<Scripts>
			<OnClick>
				self.CheckBox:SetChecked(not self.CheckBox:GetChecked());
				self:OnValueChanged(self.CheckBox:GetChecked(), true);
				PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
			</OnClick>
		</Scripts>
	</Button>
	
	<Frame name="WQT_SettingSliderTemplate" inherits="WQT_SettingsBaseTemplate" mixin="WQT_SettingsSliderMixin" virtual="true">
		<Size y="49"/>
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormal" maxLines="1" justifyH="LEFT">
					<Anchors>
						<Anchor point="TOPLEFT" x="42" y="-7"/>
						<Anchor point="RIGHT" x="-15"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>

			<Frame parentKey="SliderWithSteppers" inherits="MinimalSliderWithSteppersTemplate">
				<Size y="20"/>
				<Anchors>
					<Anchor point="BOTTOMLEFT" x="40" y="4"/>
					<Anchor point="RIGHT" x="-80"/>
				</Anchors>
			</Frame>
			<EditBox parentKey="TextBox" inherits="InputBoxTemplate, WQT_SettingsChildTemplate" autoFocus="false">
				<Size x="30" y="20"/>
				<HitRectInsets left="-10"/>
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.SliderWithSteppers" relativePoint="RIGHT" x="10" y="1"/>
					<Anchor point="RIGHT" x="-40" y="1"/>
				</Anchors>
				<FontString inherits="NumberFont_Shadow_Small"/>
				<Scripts>
					<OnEnterPressed>
						self:ClearFocus();
					</OnEnterPressed>
					<OnEditFocusLost>
						self:GetParent():OnValueChanged(self:GetText(), true);
					</OnEditFocusLost>
				</Scripts>
			</EditBox>
		</Frames>
		
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
	
	<Frame name="WQT_SettingTextTemplate" mixin="WQT_SettingsTextMixin" virtual="true">
