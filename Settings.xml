				<Texture parentKey="Background" atlas="QuestLog-main-background" setAllPoints="true">
					<TexCoords left="0.0" right="1.0" top="0.35" bottom="0.40"/>
				</Texture>
				<MaskTexture parentKey="Mask" file="Interface\Addons\WorldQuestTab\Images\RewardMask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Background" x="-50" y="0"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Background" x="50" y="-0"/>
