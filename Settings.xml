	<Button name="WQT_SettingColorTemplate" inherits="WQT_SettingsBaseTemplate, BaseLayoutFrameTemplate" mixin="WQT_SettingsColorMixin, WQT_HorizontalFlexLayoutMixin" virtual="true">
		<Size y="32"/>
		<KeyValues>
			<KeyValue key="rightPadding" value="42" type="number" />
			<KeyValue key="leftPadding" value="42" type="number" />
			<KeyValue key="spacing" value="6" type="number"/>
		</KeyValues>
		<Frames>
			<Button parentKey="Picker" inherits="UIMenuButtonStretchTemplate, WQT_SettingsChildTemplate">
				<Size x="26" y="26"/>
				<KeyValues>
					<KeyValue key="layoutIndex" value="1" type="number"/>
					<KeyValue key="align" value="center" type="string"/>
				</KeyValues>
				
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Color" atlas="LootBanner-IconGlow">
							<Size x="14" y="14"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnClick> 
						self:GetParent():StartPicking();
						PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
					</OnClick>	
					<OnMouseDown inherit="prepend">
						if (self:IsEnabled()) then
							self.Color:SetPoint("CENTER", self, 1, -1);
						end
					</OnMouseDown>
					<OnMouseUp inherit="prepend">
						if (self:IsEnabled()) then
							self.Color:SetPoint("CENTER", self, 0, 0);
						end
					</OnMouseUp>
				</Scripts>		
				<HighlightTexture atlas="bags-glow-white" alphaMode="ADD" alpha="0.5">
					<Anchors>
						<Anchor point="TOPLEFT" x="3" y="-3"/>
						<Anchor point="BOTTOMRIGHT" x="-3" y="3"/>
					</Anchors>
				</HighlightTexture>
			</Button>
			
			<Button parentKey="ResetButton" inherits="UIMenuButtonStretchTemplate" motionScriptsWhileDisabled="true" hidden="true">
				<Size x="26" y="26"/>
				<KeyValues>
					<KeyValue key="layoutIndex" value="4" type="number"/>
					<KeyValue key="align" value="center" type="string"/>
				</KeyValues>
				
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="Icon" atlas="soulbinds_tree_undo" alpha="0.75">
							<Size x="14" y="14"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Scripts>
					<OnEnter>
						self:GetParent():OnEnter(self:GetParent());
						if (self:IsEnabled()) then
							GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
							GameTooltip_SetTitle(GameTooltip, RESET_TO_DEFAULT);
							GameTooltip:Show();
						end
					</OnEnter>
					<OnLeave>
						self:GetParent():OnLeave();
						GameTooltip:Hide();
					</OnLeave>
					<OnClick> 
						self:GetParent():ResetColor(true);
						PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
					</OnClick>	
					<OnMouseDown inherit="prepend">
						if (self:IsEnabled()) then
							self.Icon:SetPoint("CENTER", self, 1, -1);
						end
					</OnMouseDown>
					<OnMouseUp inherit="prepend">
						if (self:IsEnabled()) then
							self.Icon:SetPoint("CENTER", self, 0, 0);
						end
					</OnMouseUp>
				</Scripts>		
				<HighlightTexture atlas="bags-glow-white" alphaMode="ADD" alpha="0.5">
					<Anchors>
						<Anchor point="TOPLEFT" x="3" y="-3"/>
						<Anchor point="BOTTOMRIGHT" x="-3" y="3"/>
					</Anchors>
				</HighlightTexture>
			</Button>
		</Frames>
		
		<Layers>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontNormal" maxLines="1" justifyH="LEFT">
					<KeyValues>
						<KeyValue key="layoutIndex" type="number" value="2"/>
						<KeyValue key="align" type="string" value="center"/>
						<KeyValue key="flexSize" type="number" value="1"/>
					</KeyValues>
				</FontString>
				<FontString parentKey="ExampleText" inherits="GameFontWhite" maxLines="1" justifyH="LEFT" text="PREVIEW" hidden="true">
					<KeyValues>
						<KeyValue key="layoutIndex" value="3" type="number"/>
						<KeyValue key="align" value="center" type="string"/>
						<KeyValue key="flexSize" type="number" value="1"/>
					</KeyValues>
				</FontString>	
			</Layer>
		</Layers>
		
		<Frames>
			<Button parentKey="ExampleRing" hidden="true">
				<Size x="25" y="25" />
				<KeyValues>
					<KeyValue key="layoutIndex" value="2" type="number"/>
					<KeyValue key="align" value="center" type="string"/>
				</KeyValues>
				<Layers>
					<Layer level="ARTWORK" textureSubLevel="-1">
						<!-- This serves as a background for the timer -->
						<Texture parentKey="RingBG" alpha="1" file="Interface\Addons\WorldQuestTab\Images\PoIRingBG" />
					</Layer>
					<Layer level="ARTWORK" textureSubLevel="2" >
						<!-- Edge highlight of the timer. Custom so it can be colored -->
						<Texture parentKey="Pointer" alpha="1" file="Interface\Addons\WorldQuestTab\Images\PoIRingPointer"/>
					</Layer>
				</Layers>
				<Frames>
					<Cooldown parentKey="Ring" useParentLevel="true" setAllPoints="true" hideCountdownNumbers="true">
						<SwipeTexture file="Interface\Addons\WorldQuestTab\Images\PoIRingBar"/>
					</Cooldown>
				</Frames>
			</Button>
		</Frames>
		
		<Scripts>
			<OnClick>
				self:StartPicking();
				PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
			</OnClick>	
		</Scripts>
	</Button>
	
