	<Script file="Settings.lua"/>

	<Button name="WQT_SettingCategoryTemplate" mixin="WQT_SettingsCategoryMixin" virtual="true">
		<Size y="34"/>	
		<KeyValues>
			<KeyValue key="staticLabelFont" value="true" type="boolean"/>
		</KeyValues>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="BGLeft" atlas="Options_ListExpand_Left" useAtlasSize="true">
					<Anchors>
						<Anchor point="LEFT" x="10" y="0"/>
					</Anchors>
				</Texture>
				<Texture parentKey="BGRight" atlas="Options_ListExpand_Right" useAtlasSize="true">
					<Anchors>
						<Anchor point="RIGHT" x="-10" y="0"/>
					</Anchors>
				</Texture>
				<Texture parentKey="BGMiddle" atlas="_Options_ListExpand_Middle" horizTile="true">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.BGLeft" relativePoint="TOPRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.BGRight" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
			
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontHighlightMedium" maxLines="1" justifyH="CENTER">
					<Anchors>
						<Anchor point="LEFT" x="30" y="0"/>
						<Anchor point="RIGHT" x="-30" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
			
			<Layer level="HIGHLIGHT">
				<Texture parentKey="HighlightLeft" atlas="Section_Expand_glowing_Left" useAtlasSize="true" alpha="0.7">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.BGLeft" y="-1"/>
					</Anchors>
				</Texture>
				<Texture parentKey="HighlightRight" atlas="Section_Expand_glowing_right" useAtlasSize="true" alpha="0.7">
					<Anchors>
						<Anchor point="TOPRIGHT" relativeKey="$parent.BGRight" y="-1"/>
					</Anchors>
				</Texture>
				<Texture parentKey="HighlightMiddle" atlas="Section_Expand_glowing_middle" horizTile="true" alpha="0.7">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.HighlightLeft" relativePoint="TOPRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.HighlightRight" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				self:SetExpanded(not self.isExpanded);
				PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
			</OnClick>
		</Scripts>
	</Button>
	
	<Button name="WQT_SettingSubCategoryTemplate" mixin="WQT_SettingsCategoryMixin" virtual="true">
		<Size y="36"/>	
		<KeyValues>
			<KeyValue key="staticLabelFont" value="true" type="boolean"/>
		</KeyValues>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="Background" atlas="_Options_ListExpand_Middle" alpha="0.8">
					<Anchors>
						<Anchor point="TOPLEFT" x="6" y="-5"/>
						<Anchor point="BOTTOMRIGHT" x="-6" y="5"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString parentKey="Label" inherits="GameFontHighlight" maxLines="1" justifyH="CENTER">
					<Anchors>
						<Anchor point="LEFT" x="15" y="1"/>
						<Anchor point="RIGHT" x="-15" y="1"/>
					</Anchors>
				</FontString>
				<Texture parentKey="ExpandIcon" atlas="friendslist-categorybutton-arrow-right" useAtlasSize="true" desaturated="true">
					<Anchors>
						<Anchor point="CENTER" relativePoint="RIGHT" x="-50" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" atlas="Soulbinds_Collection_Entry_Selected" alphaMode="ADD" alpha="0.4">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Background" x="20" y="-1"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Background" x="-20" y="1"/>
					</Anchors>
				</Texture>

				<MaskTexture parentKey="Mask" file="Interface\Addons\WorldQuestTab\Images\RewardMask" hWrapMode="CLAMPTOBLACKADDITIVE" vWrapMode="CLAMPTOBLACKADDITIVE">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Background" x="20" y="50"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Background" x="-20" y="-50"/>
