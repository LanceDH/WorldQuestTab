		<Scripts>
			<OnClick>
				self:SetExpanded(not self.isExpanded);
				PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);
			</OnClick>
		</Scripts>
	</Button>

	<Frame name="WQT_SettingsQuestListPreviewTemplate" mixin="WQT_SettingsQuestListMixin" virtual="true">
